#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules/webpack-dev-server/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules/webpack-dev-server/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/webpack-dev-server/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/webpack-dev-server/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules/webpack-dev-server/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules/webpack-dev-server/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/webpack-dev-server@3.11.3_webpack-cli@4.9.1+webpack@5.64.4/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/webpack-dev-server/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/webpack-dev-server/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
