#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules/testcafe/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules/testcafe/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/testcafe/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/testcafe/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules/testcafe/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules/testcafe/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/testcafe@1.17.1/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules/.pnpm/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/temp/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/common/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/testcafe/bin/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/testcafe/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../testcafe/bin/testcafe-with-v8-flag-filter.js" "$@"
else
  exec node  "$basedir/../testcafe/bin/testcafe-with-v8-flag-filter.js" "$@"
fi
