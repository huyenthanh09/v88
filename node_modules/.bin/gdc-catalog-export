#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huyennguyen/BearLiveExample/v8.8/tools/catalog-export/dist/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/tools/catalog-export/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/tools/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/catalog-export/dist/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/catalog-export/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/huyennguyen/BearLiveExample/v8.8/tools/catalog-export/dist/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/tools/catalog-export/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/tools/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/node_modules:/Users/huyennguyen/BearLiveExample/node_modules:/Users/huyennguyen/node_modules:/Users/node_modules:/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/catalog-export/dist/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/catalog-export/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules/@gooddata/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/sdk-examples/node_modules:/Users/huyennguyen/BearLiveExample/v8.8/examples/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@gooddata/catalog-export/dist/index.js" "$@"
else
  exec node  "$basedir/../@gooddata/catalog-export/dist/index.js" "$@"
fi
